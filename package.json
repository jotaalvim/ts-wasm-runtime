    "test": "turbo run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryohey/ts-wasm-runtime.git"
  },
  "author": "ryohey",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.4.4"
  },
  "packageManager": "npm@10.8.1"
}
